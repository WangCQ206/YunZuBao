.table {
  border: 1px solid darkgray;
 }
 .tr {
  display: flex;
  width: 100%;
  justify-content: center;
  align-items: center;
  border-bottom: 1px solid darkgray;
 
 }
 .td {
  width:100%;
  height: auto;
  justify-content: center;
  text-align:center;
 }
 
 .bg-w{
  background: snow;
 }
 .bg-g{
  background: #E6F3F9;
  
 }
 .th {
  width: 100%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;  
 }

